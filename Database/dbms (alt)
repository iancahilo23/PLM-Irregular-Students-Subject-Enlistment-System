-- EXCEPTIONS
CREATE OR REPLACE PROCEDURE exLogin_student (
    p_student_id IN LOGIN.student_id%TYPE
    , p_password   IN LOGIN.password%TYPE
) IS
    v_stored_password LOGIN.password%TYPE;
    e_incorrect_password EXCEPTION;
BEGIN
    SELECT password INTO v_stored_password
    FROM login
    WHERE student_id = p_student_id;

    IF v_stored_password != p_password THEN
        RAISE e_incorrect_password;
    END IF;

    DBMS_OUTPUT.PUT_LINE('Login Successful! Access Granted for: ' || p_student_id);

  EXCEPTION
    WHEN NO_DATA_FOUND THEN
        DBMS_OUTPUT.PUT_LINE('Login Failed: Student ID ' || p_student_id || ' is not registered.');

    WHEN e_incorrect_password THEN
        DBMS_OUTPUT.PUT_LINE('Login Failed: Incorrect password provided.');

    WHEN OTHERS THEN
        DBMS_OUTPUT.PUT_LINE('System Error during login: ' || SQLERRM);
END;

CREATE OR REPLACE PROCEDURE exRegister_student (
    p_student_id     IN STUDENT.student_id%TYPE
    , p_lastname       IN STUDENT.lastname%TYPE
    , p_firstname      IN STUDENT.firstname%TYPE
    , p_middlename     IN STUDENT.middlename%TYPE
    , p_birthdate      IN DATE
    , p_course_id      IN STUDENT.course_id%TYPE
    , p_year_level     IN STUDENT.year_level%TYPE
    , p_section        IN STUDENT.section%TYPE
    , p_semester       IN STUDENT.semester%TYPE
    , p_school_year    IN STUDENT.school_year%TYPE
    , p_type_id        IN STUDENT.type_id%TYPE
    , p_reg_status     IN STUDENT.registration_id%TYPE
) IS
    PRAGMA AUTONOMOUS_TRANSACTION;
BEGIN
    INSERT INTO STUDENT (
        student_id, lastname, firstname, middlename, birthdate, 
        course_id, year_level, section, semester, school_year, 
        type_id, registration_id, date_enrolled
    ) VALUES (
        p_student_id
        , p_lastname
        , p_firstname
        , p_middlename
        , p_birthdate
        , p_course_id
        , p_year_level
        , p_section
        , p_semester
        , p_school_year
        , p_type_id
        , p_reg_status
        , SYSDATE
    );

    COMMIT;
    DBMS_OUTPUT.PUT_LINE('Success: Student ' || p_student_id || ' registered.');

EXCEPTION
    WHEN DUP_VAL_ON_INDEX THEN
        DBMS_OUTPUT.PUT_LINE('Error: Student ID ' || p_student_id || ' already exists.');

        INSERT INTO log_table (log_message, log_date, log_user, log_timestamp)
        VALUES (
            'FAILED INSERT: Duplicate ID ' || p_student_id || ' for Name: ' || p_lastname
            , SYSDATE
            , USER
            , SYSTIMESTAMP
        );
        COMMIT;

    WHEN OTHERS THEN
        DBMS_OUTPUT.PUT_LINE('System Error: ' || SQLERRM);
        ROLLBACK;
END;

CREATE OR REPLACE PROCEDURE exRegister_faculty (
    p_faculty_id       IN FACULTY.faculty_id%TYPE
    , p_lastname       IN FACULTY.lastname%TYPE
    , p_firstname      IN FACULTY.firstname%TYPE
    , p_college_id     IN FACULTY.college_id%TYPE
) IS
    PRAGMA AUTONOMOUS_TRANSACTION;
BEGIN
    INSERT INTO FACULTY (
        faculty_id, lastname, firstname, college_id
    ) VALUES (
        p_faculty_id
        , p_lastname
        , p_firstname
        , p_college_id
    );

    COMMIT;
    DBMS_OUTPUT.PUT_LINE('Success: Faculty ' || p_faculty_id || ' registered.');

EXCEPTION
    WHEN DUP_VAL_ON_INDEX THEN
        DBMS_OUTPUT.PUT_LINE('Error: Faculty ID ' || p_faculty_id || ' already exists.');

        INSERT INTO log_table (log_message, log_date, log_user, log_timestamp)
        VALUES (
            'FAILED INSERT: Duplicate Faculty ID ' || p_faculty_id || ' for Name: ' || p_lastname
            , SYSDATE
            , USER
            , SYSTIMESTAMP
        );
        COMMIT;

    WHEN OTHERS THEN
        DBMS_OUTPUT.PUT_LINE('System Error: ' || SQLERRM);
        ROLLBACK;
END;
