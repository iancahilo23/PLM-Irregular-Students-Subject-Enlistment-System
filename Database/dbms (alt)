-- COURSE TYPE TABLE

CREATE TABLE course_type(
    course_type_id          VARCHAR(5)           NOT NULL,
    course_type_title     VARCHAR(75)          DEFAULT ' ',
    CONSTRAINT            pk_course_type_id    PRIMARY KEY (course_type_id)
); 

INSERT INTO course_type VALUES ('UNDER', 'Undergraduate Programs');
INSERT INTO course_type VALUES ('GRAD', 'Graduate Programs');

-- COURSE TABLE (BSIT, BSCS, etc.)

CREATE TABLE COURSE(
    course_id           VARCHAR(15)       NOT NULL,
    course_title      VARCHAR(100)      DEFAULT ' ',
    course_type_id    VARCHAR(5)        DEFAULT ' ',
    college_id        VARCHAR(5)        NOT NULL,
    CONSTRAINT        pk_course_id      PRIMARY KEY (course_id),
    CONSTRAINT        fk_college_id     FOREIGN KEY (college_id)     REFERENCES i_COLLEGE (college_id),
    CONSTRAINT        fk_course_type_id FOREIGN KEY (course_type_id) REFERENCES i_COURSE_TYPE (course_type_id)
);

INSERT INTO course VALUES ('BSIT', 'Bachelor of Science in Information Technology', 'UNDER', 'CISTM');
