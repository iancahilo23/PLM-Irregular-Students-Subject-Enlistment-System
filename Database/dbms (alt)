-- LOGIN TABLE
CREATE TABLE login (
    student_id       VARCHAR(50)       NOT NULL,
    password       VARCHAR(50)      DEFAULT '1234' NOT NULL,
    CONSTRAINT     pk_student_login  PRIMARY KEY (student_id),
    CONSTRAINT     fk_student_login  FOREIGN KEY (student_id) REFERENCES i_STUDENT (student_id) ON DELETE CASCADE
);

