-- FACULTY TABLE

CREATE TABLE FACULTY (
    faculty_id           VARCHAR(50)  NOT NULL,
    lastname             VARCHAR(100) DEFAULT ' ',
    firstname            VARCHAR(100) DEFAULT ' ',
    college_id           VARCHAR(50),
    CONSTRAINT pk_ifaculty_id PRIMARY KEY(faculty_id),
    CONSTRAINT fk_icollege_id FOREIGN KEY(college_id) REFERENCES i_COLLEGE(college_id)
);

