-- SUBJECT TABLE

CREATE TABLE subject (
    subject_id        VARCHAR(50)      NOT NULL,
    subject_title     VARCHAR(150)     NOT NULL,
    lec_hours         VARCHAR(15)      NOT NULL,
    lab_hours         VARCHAR(15)      NOT NULL,
    units             DECIMAL(4,1)     NOT NULL,
    CONSTRAINT sub_id_pk              PRIMARY KEY (subject_id)
);

-- SCHEDULES AND SECTIONS

CREATE TABLE subject_section (
    section_id        INT,
    subject_id        VARCHAR(50),
    day_of_week       VARCHAR(10),
    time_start        VARCHAR(50),
    time_end          VARCHAR(50),
    room              VARCHAR(50),
    faculty_id        VARCHAR(50),
    CONSTRAINT section_pk PRIMARY KEY(section_id, subject_id, day_of_week),
    CONSTRAINT sec_subject_fk FOREIGN KEY (subject_id) REFERENCES i_subject(subject_id),
    CONSTRAINT sec_faculty_fk FOREIGN KEY (faculty_id) REFERENCES i_faculty(faculty_id)
);

