-- STUDENT_TYPE TABLE

CREATE TABLE STUDENT_TYPE (
    type_id                VARCHAR(50)  NOT NULL
    , type_title           VARCHAR(150) DEFAULT ' '
    , CONSTRAINT pk_type_id PRIMARY KEY(type_id)
    , CONSTRAINT fk_student_id FOREIGN KEY(student_id) REFERENCES STUDENT(student_id)
);

INSERT INTO STUDENT_TYPE VALUES ('N','New');
INSERT INTO STUDENT_TYPE VALUES ('O','Old');

-- REGISTRATION_STATUS TABLE

CREATE TABLE REGISTRATION_STATUS (
    registration_id        VARCHAR(50)  NOT NULL
    , registration_title   VARCHAR(150) DEFAULT ' '
    , CONSTRAINT pk_registration_id PRIMARY KEY(registration_id)
    , CONSTRAINT fk_student_id FOREIGN KEY(student_id) REFERENCES STUDENT(student_id)
);

INSERT INTO REGISTRATION_STATUS VALUES ('REG','Regular');
INSERT INTO REGISTRATION_STATUS VALUES ('IRR','Irregular');

-- ENROLLMENT_STATUS TABLE

CREATE TABLE ENROLLMENT_STATUS (
    enroll_id              VARCHAR(50)  NOT NULL
    , enroll_title         VARCHAR(150) DEFAULT ' '
    , CONSTRAINT pk_enroll_id PRIMARY KEY(enroll_id)
    , CONSTRAINT fk_subject_id FOREIGN KEY(subject_id) REFERENCES SUBJECT(subjectt_id)
);

INSERT INTO ENROLLMENT_STATUS VALUES ('ENR','Enrolled');
INSERT INTO ENROLLMENT_STATUS VALUES ('PEN','Pending');
