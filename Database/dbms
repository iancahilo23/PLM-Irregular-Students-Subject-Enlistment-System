--- STUDENT-SUBJECT ENROLLMENT
    student_id            VARCHAR(15)
  , subject_id            VARCHAR(15)
  , CONSTRAINT enroll_pk PRIMARY KEY(student_id, subject_id)
  , CONSTRAINT enstud_fk FOREIGN KEY (student_id) REFERENCES student(student_id)
  , CONSTRAINT ensubj_fk FOREIGN KEY (subject_id) REFERENCES subject(subject_id)
);



--SCHEDULES AND OPTIONS

CREATE TABLE schedule (
    sched_id            VARCHAR(15)
  , day_of_week         VARCHAR(5)
  , time_start          VARCHAR(10)
  , time_end            VARCHAR(10)
  , CONSTRAINT sched_pk PRIMARY KEY(sched_id)
);

CREATE TABLE schedule_option (
    sched_option_id           VARCHAR(15)
  , sched_id                  VARCHAR(15)
  , faculty_id                VARCHAR(15)
  , CONSTRAINT schedopt_pk PRIMARY KEY(sched_option_id)
  , CONSTRAINT sosched_fk FOREIGN KEY (sched_id) REFERENCES schedule(sched_id)
  , CONSTRAINT sofaculty_fk FOREIGN KEY (faculty_id) REFERENCES faculty(faculty_id)
);

