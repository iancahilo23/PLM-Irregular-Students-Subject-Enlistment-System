--COURSE TYPE TABLE 

CREATE TABLE course_type(
    course_type_id          VARCHAR(5)           NOT NULL
    , course_type_title     VARCHAR(75)          DEFAULT ' '
    , CONSTRAINT            pk_course_type_id    PRIMARY KEY (course_type_id)
);

INSERT INTO course_type VALUES ('UNDER', 'Undergraduate Programs');
INSERT INTO course_type VALUES ('GRAD', 'Graduate Programs');


--STUDENT TABLE

CREATE TABLE STUDENT (
    student_id           VARCHAR(50)         NOT NULL
    , lastname           VARCHAR(100)        DEFAULT ' '
    , firstname          VARCHAR(100)        DEFAULT ' '
    , middlename         VARCHAR(100)        DEFAULT ' '
    , course_id          VARCHAR(50)         NOT NULL
    , year_level         INT                 NOT NULL
    , section            INT                 NOT NULL
    , semester           VARCHAR(50)         NOT NULL
    , school_year        VARCHAR(50)         NOT NULL
    , date_enrolled      DATE                DEFAULT NULL
    , type_id            VARCHAR(50)
    , registration_id    VARCHAR(50)
    , CONSTRAINT pk_student_id PRIMARY KEY (student_id)
    , CONSTRAINT fk_student_type_id FOREIGN KEY (type_id) REFERENCES STUDENT_TYPE(type_id)
    , CONSTRAINT fk_student_registration_id FOREIGN KEY (registration_id) REFERENCES REGISTRATION_STATUS(registration_id)
      
);

