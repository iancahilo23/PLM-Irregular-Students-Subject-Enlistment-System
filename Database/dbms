--SCHEDULES AND OPTIONS

CREATE TABLE schedule (
    sched_id            VARCHAR(15)    NOT NULL
  , day_of_week         VARCHAR(5)     NOT NULL
  , time_start          TIME           NOT NULL
  , time_end            TIME           NOT NULL
  , CONSTRAINT sched_pk PRIMARY KEY(sched_id)
);

CREATE TABLE schedule_option (
    sched_option_id           VARCHAR(15)
  , sched_id                  VARCHAR(15)
  , faculty_id                VARCHAR(15)
  , CONSTRAINT schedopt_pk PRIMARY KEY(sched_option_id)
  , CONSTRAINT sosched_fk FOREIGN KEY (sched_id) REFERENCES schedule(sched_id)
  , CONSTRAINT sofaculty_fk FOREIGN KEY (faculty_id) REFERENCES faculty(faculty_id)
);

