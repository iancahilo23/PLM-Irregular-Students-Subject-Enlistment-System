--COURSE TABLE (BSIT, BSCS, etc.)

CREATE TABLE COURSE(
    course_id           VARCHAR(15)       NOT NULL
    , course_title      VARCHAR(100)      DEFAULT ' '
    , course_type_id    VARCHAR(5)        DEFAULT ' '
    , college_id        VARCHAR(5)        NOT NULL
    , CONSTRAINT        pk_course_id      PRIAMRY KEY (course_id)
    , CONSTRAINT        fk_college_id     FOREIGN KEY (college_id)     REFERENCES COLLEGE (college_id)
    , CONSTRAINT        fk_course_type_id FOREIGN KEY (course_type_id) REFERENCES COURSE_TYPE (course_type_id)
);

