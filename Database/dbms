--SUBJECT TABLE

CREATE TABLE subject (
    subject_id        VARCHAR(50)      NOT NULL,
    subject_title     VARCHAR(150)     NOT NULL,
    lec_hours         TIME             NOT NULL,
    lab_hours         TIME             NOT NULL,
    units             DECIMAL(4,1)     NOT NULL,
    sched_option_id   VARCHAR(50),
    enroll_id         VARCHAR(50),
    CONSTRAINT subject_id_pk              PRIMARY KEY (subject_id),
    CONSTRAINT subject_sched_option_id_fk FOREIGN KEY (sched_option_id) REFERENCES schedule_option(sched_option_id),
    CONSTRAINT subject_enroll_id_fk       FOREIGN KEY (enroll_id)       REFERENCES enrollment_status(enroll_id)
);

